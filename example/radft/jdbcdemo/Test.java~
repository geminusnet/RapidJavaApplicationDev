      nBCachedRowSet1.setConnectionSource(connectionSource1);
      nBJdbcRowSet1.setCommand("select * from Address;");
      nBJdbcRowSet1.setConnectionSource(connectionSource1);
      
      dataNavigator1.setRowSet(nBJdbcRowSet1);
      getContentPane().add(dataNavigator1, java.awt.BorderLayout.NORTH);
      
