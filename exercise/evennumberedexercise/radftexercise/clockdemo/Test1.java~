import java.awt.image.*;
import java.awt.*;

public class Test1 {
  public static void main(String[] args) {

    Image image = new BufferedImage(300, 200, BufferedImage.TYPE_INT_ARGB);
    Graphics g = image.getGraphics();
    Clock clock = new Clock();
    clock.paint(g);
  }
}